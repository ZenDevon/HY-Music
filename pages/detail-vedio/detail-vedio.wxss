/* pages/detail-vedio/detail-vedio.wxss */
page {
  height: 100vh;
}
.video {
  width: 100%;
}

.videoInfo {
  box-sizing: border-box;
  padding: 30rpx 20rpx;
}
.videoInfo .videoName {
  font-size: 38rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.videoInfo .artistName {
  font-size: 30rpx;
  color: rgb(87, 85, 85);
  margin-bottom: 20rpx;
}
.videoInfo .other {
  display: flex;
  font-size: 30rpx;
  color: rgb(87, 85, 85);
}
.videoCommend {
  box-sizing: border-box;
  padding: 0 20rpx;
}
.videoCommend .item {
  margin-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
}
.videoCommend .title {
  font-size: 36rpx;
  margin-bottom: 20rpx;
}
.videoCommend .item .album {
  width: 380rpx;
  height: 160rpx;
  border-radius: 13rpx;
  overflow: hidden;
  position: relative;
}
.videoCommend .item .album .paycount {
  position: absolute;
  right: 5px;
  bottom: 8rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: #fff;
}
.videoCommend .item .album .paycount::before {
  content: "";
  position: absolute;
  left: -40rpx;
  top: -1rpx;
  width: 30rpx;
  height: 30rpx;
  background-size: cover;
  background-image: url("data:image/png;base64,");
}
.videoCommend .item .album .image {
  width: 100%;
  height: 100%;
}
.videoCommend .item .commendItem {
  margin-left: 14rpx;
  font-size: 30rpx;
}
.videoCommend .item .commendItem .commendTitle {
  color: #333;
  font-weight: bold;
   /* 显示两行 */
   overflow: hidden;
   text-overflow: ellipsis;
   display: -webkit-box;
   -webkit-line-clamp: 2;
   -webkit-box-orient: vertical;
}
.videoCommend .item .commendItem .artistName {
  margin-top: 12rpx;
  font-size: 28rpx;
  color: rgb(87, 85, 85);
}

.content {
  height: calc(100% 225px);
}